<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="local-analyzers" value="../WorkTrack.Common.Analyzers/nupkg" />
    <add key="github" value="https://nuget.pkg.github.com/WorkTrack-Team/index.json" />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="local-analyzers">
      <package pattern="WorkTrack.Common.Analyzers" />
    </packageSource>
    <packageSource key="github">
      <package pattern="WorkTrack.Common.*" />
    </packageSource>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
